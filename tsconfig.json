{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",  // Cambia esto a 'esnext' o alguno de los otros m√≥dulos permitidos
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react-jsx"                           
  },
  "include": ["src/**/*"],  // Incluir todos los archivos en la carpeta src
  "exclude": ["node_modules"]  // Excluir node_modules
}
